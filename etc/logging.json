{
    "loggers": {
        "buildings": {
            "handlers": [
                "buildings",
                "buildings_logstash",
                "console"
            ], 
            "level": "DEBUG"
        },
        "model": {
            "handlers": [
                "model",
                "model_logstash",
                "console"
            ], 
            "level": "DEBUG"
        },
        "token": {
            "handlers": [
                "token",
                "token_logstash",
                "console"
            ], 
            "level": "DEBUG"
        }
     },
     "handlers": {
		"buildings": {
            "backupCount": 5, 
            "level": "DEBUG", 
            "filename": "BASE/buildings.log", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "default", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
	    "buildings_logstash": {
            "backupCount": 2, 
            "level": "DEBUG", 
            "filename": "BASE/buildings.json", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "json", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
        "model": {
            "backupCount": 5, 
            "level": "DEBUG", 
            "filename": "BASE/model.log", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "default", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
	    "model_logstash": {
            "backupCount": 2, 
            "level": "DEBUG", 
            "filename": "BASE/model.json", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "json", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
        "token": {
            "backupCount": 5, 
            "level": "DEBUG", 
            "filename": "BASE/token.log", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "default", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
	    "token_logstash": {
            "backupCount": 2, 
            "level": "DEBUG", 
            "filename": "BASE/token.json", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "json", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
        "console": {
            "formatter": "default", 
            "class": "logging.StreamHandler", 
            "stream": "ext://sys.stdout", 
            "level": "DEBUG"
        } 
 	},
    "version": 1, 
    "filters": {}, 
    "formatters": {
        "default": {
            "datefmt": "%Y.%m.%dT%H:%M:%S", 
            "format": "%(asctime)s %(levelname)-8s %(name)-15s %(message)s"
        }, 
        "json": {
            "()": "logstash_formatter.LogstashFormatter"
        }, 
        "dateTime": {
            "datefmt": "%Y.%m.%dT%H:%M:%S", 
            "format": "%(asctime)s %(levelname)-8s %(name)-15s %(message)s"
        }
    }
}
        