{
    "loggers": {
        "buildings": {
            "handlers": [
                "buildings", 
                "console"
            ], 
            "level": "DEBUG"
        }
     },
     "handlers": {
        "buildings": {
            "backupCount": 5, 
            "level": "DEBUG", 
            "filename": "/vol/d2l/var/log/buildings.log", 
            "maxBytes": 10485760, 
            "mode": "a", 
            "formatter": "default", 
            "class": "logging.handlers.RotatingFileHandler"
        }, 
        "console": {
            "formatter": "default", 
            "class": "logging.StreamHandler", 
            "stream": "ext://sys.stdout", 
            "level": "DEBUG"
        } 
 	},
    "version": 1, 
    "filters": {}, 
    "formatters": {
        "default": {
            "datefmt": "%Y.%m.%dT%H:%M:%S", 
            "format": "%(asctime)s %(levelname)-8s %(name)-15s %(message)s"
        }, 
        "dateTime": {
            "datefmt": "%Y.%m.%dT%H:%M:%S", 
            "format": "%(asctime)s %(levelname)-8s %(name)-15s %(message)s"
        }
    }
}
        